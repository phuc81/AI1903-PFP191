# print the sum of current number and the previuos number
# Program 1
def print_sum():
    current_number = 1
    previous_number = 0
    print(current_number)
    for i in range(9):
        sum = current_number + previous_number
        print(sum)
        previous_number = current_number
        current_number = sum

# Program 2
def iterate_and_print_sum():
    current_number = 1
    previous_number = 0
    for i in range(10):
        sum = current_number + previous_number
        print(sum)
        previous_number = current_number
        current_number = sum
